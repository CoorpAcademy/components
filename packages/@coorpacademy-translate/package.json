  ],
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@coorpacademy/eslint-plugin-coorpacademy": "^9.0.1",
