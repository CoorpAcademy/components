    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-polyfill": "^6.13.0",
    "history": "^3.2.1",
    "lodash": "^4.15.0",
    "path-match": "^1.2.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "redux": "^3.6.0",
    "redux-api-middleware": "^2.0.0-beta.1",
    "rxjs": "^5.0.0-beta.12",
    "validator": "^6.0.0"
