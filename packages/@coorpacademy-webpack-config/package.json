{
  "name": "@coorpacademy/webpack-config",
  "version": "7.0.5",
  "description": "",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env babel src --out-dir lib -s --ignore test --copy-files",
    "ava": "cross-env NODE_ENV=test ava",
    "nyc": "cross-env NODE_ENV=test nyc npm run ava",
    "test": "npm run lint",
    "lint": "npm run eslint",
    "eslint": "eslint ./ *.js *.json --ext .js,.json",
    "clean:build": "rimraf lib",
    "clean": "npm run clean:build",
    "prepare": "npm run clean && npm run build --production"
  },
  "contributors": [
    "Adrien Becchis <adrien.becchis@coorpacademy.com>",
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>",
    "Gaelle Leguet <gaelle.leguet@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Joan Ortega <joan.ortega@coorpacademy.com>",
    "Julien Seren-Rosso <julien.seren-rosso@coorpacademy.com>"
  ],
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.5.0",
    "nyc": "^13.1.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.1",
    "cross-env": "^5.0.0",
    "eslint": "^5.15.3",
    "rimraf": "^2.6.1",
    "webpack": "^4.8.2"
  },
  "author": "CoorpAcademy"
}
