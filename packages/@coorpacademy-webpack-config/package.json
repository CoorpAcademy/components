{
  "name": "@coorpacademy/webpack-config",
  "version": "7.0.3",
  "description": "",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env babel src --out-dir lib -s --ignore test --copy-files",
    "ava": "cross-env NODE_ENV=test ava",
    "nyc": "cross-env NODE_ENV=test nyc npm run ava",
    "test": "npm run lint",
    "lint": "npm run eslint",
    "eslint": "eslint ./ *.js *.json --ext .js,.json",
    "clean:build": "rimraf lib",
    "clean": "npm run clean:build",
    "prepare": "npm run clean && npm run build --production"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>",
    "Gaelle Leguet <gaelle.leguet@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Joan Ortega <joan.ortega@coorpacademy.com>",
    "Julien Seren-Rosso <julien.seren-rosso@coorpacademy.com>"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.11",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.0.5",
    "style-loader": "^0.21.0",
    "webpack-bundle-analyzer": "^2.11.2"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "cross-env": "^5.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^4.8.2"
  },
  "author": "CoorpAcademy"
}
