{
  "presets": [
    "@babel/preset-react"
  ],
  "plugins": ["lodash"],
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": true
            },
            "loose": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": true
            },
            "loose": true
          }
        ]
      ],
      "plugins": [
        "istanbul"
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": true
            },
            "modules": false,
            "loose": true
          }
        ]
      ]
    }
  }
}
