{
  "include": [
    "src/**"
  ],
  "exclude" : [
    "src/index.js",
    "src/start.js",
    "**/test/",
    "**/es/",
    "**/lib/"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "check-coverage": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "cache": true,
  "require": "babel-register",
  "sourceMap": false,
  "instrument": false,
  "all": true
}
