{
  "name": "@coorpacademy/player-store",
  "version": "1.0.0",
  "description": "Redux store for slide players",
  "main": "lib/web/index.js",
  "module": "es/web/index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "npm run build:commonjs && npm run build:es",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean": "npm run clean:build:commonjs && npm run clean:build:es",
    "prepare": "npm run clean && npm run build --production"
  },
  "homepage": "https://github.com/CoorpAcademy/components/packages/@coorpacademy-player-store#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "browser-env": "^3.2.5",
    "cross-env": "^5.0.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "@coorpacademy/redux-task": "0.0.7",
    "@coorpacademy/translate": "5.3.12",
    "lodash": "^4.17.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
