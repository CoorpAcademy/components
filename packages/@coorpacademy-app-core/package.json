{
  "name": "@coorpacademy/app-core",
  "version": "5.3.1",
  "description": "CoorpAcademy redux apps core",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "npm run build:commonjs && npm run build:es",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "npm run clean:build:commonjs && npm run clean:build:es",
    "clean": "npm run clean:build",
    "prepublish": "npm run build --production",
    "prebuild:commonjs": "npm run clean:build:commonjs",
    "prebuild:es": "npm run clean:build:es"
  },
  "homepage": "https://github.com/CoorpAcademy/components/@coorpacademy-app-core#readme",
  "dependencies": {
    "@coorpacademy/components": "5.3.1",
    "@coorpacademy/history": "5.3.0",
    "@coorpacademy/redux-history": "5.3.1",
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-1": "^6.5.0",
    "lodash": "^4.15.0",
    "path-match": "^1.2.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "redux": "^3.6.0",
    "redux-api-middleware": "^2.0.0-beta.1",
    "rxjs": "^5.0.0-beta.12",
    "validator": "^6.0.0",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^3.0.0",
    "cross-env": "^4.0.0",
    "rimraf": "^2.6.1"
  }
}
