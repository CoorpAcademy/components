{
  "name": "@coorpacademy/app-player",
  "version": "7.2.7",
  "description": "CoorpAcademy player [freerun, learner, battles]",
  "main": "lib/web/index.js",
  "module": "es/web/index.js",
  "files": [
    "es",
    "lib",
    "locales",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "npm run build:commonjs && npm run build:es",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "npm run clean:build:commonjs && npm run clean:build:es",
    "clean:static": "npm run static",
    "clean": "npm run clean:build && npm run clean:static",
    "prepare": "npm run clean && npm run build --production",
    "static": "webpack --progress --output-path static/dist/ --config sandbox-web/webpack.config.js",
    "poststatic": "cp sandbox-web/index.html static/index.html",
    "start": "cross-env BABEL_ENV=es webpack-dev-server --config sandbox-web/webpack.config.js --content-base sandbox-web --host 0.0.0.0 --history-api-fallback true --hot true"
  },
  "homepage": "https://github.com/CoorpAcademy/components/packages/@coorpacademy-app-player#readme",
  "devDependencies": {
    "@coorpacademy/progression-engine": "8.1.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "browser-env": "^2.0.31",
    "cross-env": "^5.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@coorpacademy/components": "5.4.134",
    "@coorpacademy/translate": "5.3.11",
    "@coorpacademy/webpack-config": "6.2.2",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
