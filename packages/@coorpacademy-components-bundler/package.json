{
  "name": "@coorpacademy/components-bundler",
  "version": "3.0.0-beta.14",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "npm run clean:build:commonjs && BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "npm run clean:build:es && BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "prepublish": "npm run build --production",
    "clean": "npm run clean:build && npm run clean:cov",
    "clean:build": "npm run clean:build:commonjs && npm run clean:build:es && npm run clean:build:browser",
    "clean:build:commonjs": "rm -rf lib",
    "clean:build:es": "rm -rf es",
    "clean:cov": "rm -rf .nyc_output coverage",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint src/",
    "ava": "NODE_ENV=test ava",
    "nyc": "NODE_ENV=test nyc npm run ava"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-preset-stage-1": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "minimist": "^1.2.0",
    "postcss-loader": "^0.13.0",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.25"
  },
  "bin": {
    "bundle-components": "./src/index.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "ava": "^0.16.0",
    "babel-plugin-istanbul": "^2.0.1",
    "eslint": "^3.5.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.2",
    "nyc": "^8.1.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "src/**/test/**"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  },
  "author": "CoorpAcademy"
}
