{
  "include": [
    "src/**"
  ],
  "exclude" : [
    "**/test/",
    "**/es/",
    "**/lib/"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "check-coverage": true,
  "lines": 100,
  "statements": 100,
  "require": "@babel/register",
  "functions": 100,
  "branches": 100,
  "cache": true,
  "sourceMap": false,
  "instrument": false,
  "all": true
}
