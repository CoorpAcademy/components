{
  "name": "@coorpacademy/nova-icons",
  "version": "1.2.8",
  "description": "React components bank based on Nova SVG icons",
  "main": "index.js",
  "files": [
    "line/",
    "solid/",
    "composition/"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "npm run eslint",
    "eslint": "eslint ./ *.js *.json --ext .js,.json",
    "clean": "rm -rf src solid line composition",
    "generate-icons": "node scripts/generate-icons.js",
    "build": "cross-env BABEL_ENV=commonjs babel src/solid --out-dir solid && cross-env BABEL_ENV=commonjs babel src/line --out-dir line && cross-env BABEL_ENV=commonjs babel src/composition --out-dir composition",
    "prepare": "npm run generate-icons && npm run build"
  },
  "author": "CoorpAcademy",
  "dependencies": {
    "async": "^2.4.1",
    "react": "^16.3.2"
  },
  "devDependencies": {
    "@svgr/core": "^4.1.0",
    "@svgr/plugin-prettier": "^4.0.3",
    "@svgr/plugin-svgo": "^4.0.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react-native": "^4.0.1",
    "chalk": "^2.4.1",
    "change-case": "^3.0.1",
    "cross-env": "^5.0.0",
    "fs-extra": "^6.0.1",
    "minimist": "^1.2.0",
    "mkdirp-promise": "^5.0.1",
    "sax": "^1.2.2",
    "svgo": "^1.0.5",
    "walk-promise": "^0.2.0"
  }
}
