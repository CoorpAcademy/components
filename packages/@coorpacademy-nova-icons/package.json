{
  "name": "@coorpacademy/nova-icons",
  "version": "1.2.8",
  "description": "React components bank based on Nova SVG icons",
  "main": "index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "npm run eslint",
    "eslint": "eslint ./ *.js *.json --ext .js,.json",
    "clean": "rm -rf src solid line composition",
    "generate-icons": "node scripts/generate-icons.js",
    "build:babel:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:babel:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build:babel": "concurrently \"npm run build:babel:commonjs\" \"npm run build:babel:es\"",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "concurrently \"npm run clean:build:commonjs\" \"npm run clean:build:es\"",
    "prepare": "npm run generate-icons && npm run build"
  },
  "author": "CoorpAcademy",
  "dependencies": {
    "async": "^2.4.1",
    "react": "^16.3.2"
  },
  "devDependencies": {
    "@svgr/core": "^4.1.0",
    "@svgr/plugin-prettier": "^4.0.3",
    "@svgr/plugin-svgo": "^4.0.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react-native": "^4.0.1",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.0.0",
    "mkdirp": "^0.5.1"
  }
}
