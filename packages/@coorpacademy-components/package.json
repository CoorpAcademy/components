  "scripts": {
    "build:commonjs:babel": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --copy-files --no-copy-ignored --ignore \"**/test\" --extensions \".ts,.tsx,.js\"",
    "build:commonjs:types": "tsc -p tsconfig.lib.json || true",
    "build:es:babel": "cross-env BABEL_ENV=es babel src --out-dir es -s --copy-files --no-copy-ignored --ignore \"**/test\" --extensions \".ts,.tsx,.js\"",
    "build:es:types": "tsc -p tsconfig.es.json || true",
    "build:commonjs": "npm run build:commonjs:babel && npm run build:commonjs:types",
    "build:es": "npm run build:es:babel && npm run build:es:types",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "build:watch": "npm run build:es -- -w --verbose",
    "build:stats": "webpack --profile --json=dist/stats.json",
    "clean:commonjs": "rm -rf lib",
    "clean:es": "rm -rf es",
    "clean": "concurrently \"npm run clean:commonjs\" \"npm run clean:es\"",
    "generate:fixtures": "node scripts/fixture-tests.js src src/test/helpers/render-component.js",
    "generate:storybook": "node scripts/storybook-index.js src",
    "generate": "concurrently \"npm run generate:storybook\" \"npm run generate:fixtures\"",
    "prepare": "npm run clean && npm run build --production",
    "prestart": "npm run generate:storybook",
    "start": "npm run storybook:run",
    "start:ie": "IE=true npm start",
    "prestatic": "npm run generate:storybook",
    "static": "npm run storybook:export",
    "storybook:run": "cross-env BABEL_ENV=es start-storybook -p 3004 -c .storybook",
    "storybook:export": "cross-env BABEL_ENV=es build-storybook -c .storybook -o static",
    "preava": "npm run generate:fixtures",
    "ava": "ava",
    "lint": "npm run eslint && npm run typecheck",
    "lint:fix": "npm run eslint -- --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "nyc npm run ava",
    "eslint": "eslint . --ext .js,.json,.ts,.tsx --cache --cache-location \"node_modules/.cache/.eslintcache\"",
    "typecheck": "tsc --noEmit true",
    "chromatic": "npx chromatic --build-script-name storybook:export"
  },
