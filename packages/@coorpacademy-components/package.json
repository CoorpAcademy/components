{
  "name": "@coorpacademy/components",
  "version": "4.3.12",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "locales",
    "postcss.config.js",
    "README.md"
  ],
  "scripts": {
    "prestart": "npm run generate",
    "start": "BABEL_ENV=commonjs babel-node demo/index.js",
    "generate": "npm run generate:index && npm run generate:demo",
    "generate:index": "BABEL_ENV=commonjs babel-node scripts/generate-components-index.js > src/index.js",
    "generate:demo": "BABEL_ENV=commonjs babel-node scripts/generate-demo-index.js > demo/components.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:browser",
    "build:commonjs": "npm run clean:build:commonjs && BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "npm run clean:build:es && BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build:browser": "npm run clean:build:browser && npm run build:components",
    "build:components": "bundle-components src/index.js -o dist/",
    "prepublish": "npm run build --production",
    "clean": "npm run clean:build",
    "clean:build": "npm run clean:build:commonjs && npm run clean:build:es && npm run clean:build:browser",
    "clean:build:commonjs": "rm -rf lib",
    "clean:build:es": "rm -rf es",
    "clean:build:browser": "rm -rf dist/*",
    "doc": "npm run doc:svg && npm run doc:png",
    "doc:svg": "node scripts/madge.js",
    "doc:png": "convert doc/components.svg doc/components.png"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "dependencies": {
    "api-check": "^7.5.5",
    "extended-proptypes": "^1.1.4",
    "file-loader": "^0.9.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.14.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@coorpacademy/components-bundler": "^4.1.2",
    "@coorpacademy/history": "^3.7.3",
    "@coorpacademy/translate": "^4.1.2",
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "glob": "^7.0.5",
    "json-loader": "^0.5.4",
    "madge": "^1.3.0",
    "path-match": "^1.2.4",
    "postcss-loader": "^1.1.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.0",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "author": "CoorpAcademy"
}
