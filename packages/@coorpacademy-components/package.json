{
  "name": "@coorpacademy/components",
  "version": "9.0.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "locales",
    "postcss.config.js",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "concurrently \"npm run clean:build:commonjs\" \"npm run clean:build:es\"",
    "clean": "npm run clean:build",
    "doc:png": "convert doc/components.svg doc/components.png",
    "doc:svg": "node scripts/madge.js",
    "doc": "concurrently \"npm run doc:svg\" \"npm run doc:png\"",
    "generate:fixtures": "node scripts/fixture-tests.js src src/test/helpers/render-component.js",
    "generate:storybook": "node scripts/storybook-index.js src storybook/components.js > storybook/components.js",
    "generate": "concurrently \"npm run generate:storybook\" \"npm run generate:fixtures\"",
    "prepare": "npm run clean && npm run build --production",
    "prestart": "npm run generate:storybook",
    "start": "npm run storybook:run",
    "static": "npm run storybook:export",
    "storybook:run": "cross-env BABEL_ENV=es start-storybook -p 3004 -c .storybook",
    "storybook:export": "cross-env BABEL_ENV=es build-storybook -c .storybook -o static",
    "ava": "cross-env NODE_ENV=test ava",
    "nyc": "cross-env NODE_ENV=test nyc npm run ava",
    "test": " npm run lint && npm run nyc",
    "lint": "npm run eslint",
    "eslint": "eslint ./ *.js *.json --ext .js,.json"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>",
    "Gaelle Leguet <gaelle.leguet@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Joan Ortega <joan.ortega@coorpacademy.com>",
    "Julien Seren-Rosso <julien.seren-rosso@coorpacademy.com>",
    "Adrien Becchis <adrien.becchis@coorpacademy.com>"
  ],
  "dependencies": {
    "@coorpacademy/nova-icons": "3.0.0",
    "autoprefixer": "^8.4.1",
    "classnames": "^2.2.5",
    "css-color-function": "^1.3.0",
    "cubic-bezier": "^0.1.2",
    "extended-proptypes": "^1.3.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-modules-values-replace": "^3.0.1",
    "qs": "6.5.2",
    "react": "^16.3.2",
    "react-jw-player": "1.18.0",
    "react-tooltip": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-transform-block-scoping": "^7.4.0",
    "@babel/plugin-transform-destructuring": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@coorpacademy/css-modules-require-hook": "2.0.0",
    "@coorpacademy/translate": "6.0.0",
    "@coorpacademy/webpack-config": "8.0.0",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "ava": "^1.4.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "browser-env": "^3.2.4",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.18.2",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "madge": "^3.0.1",
    "nyc": "^13.3.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.0",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.1",
    "rxjs": "^6.1.0",
    "style-loader": "^0.21.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.8.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "@babel/polyfill",
      "../../test/helpers/throw-warning",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-proposal-export-default-from",
          "@babel/plugin-proposal-export-namespace-from",
          "@babel/plugin-proposal-object-rest-spread",
          "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
          "@ava/stage-4",
          "@babel/preset-react"
        ]
      }
    }
  },
  "author": "CoorpAcademy"
}
