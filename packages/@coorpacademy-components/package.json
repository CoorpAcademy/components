{
  "name": "@coorpacademy/components",
  "version": "6.0.11",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "locales",
    "postcss.config.js",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "concurrently \"npm run clean:build:commonjs\" \"npm run clean:build:es\"",
    "clean": "npm run clean:build",
    "doc:png": "convert doc/components.svg doc/components.png",
    "doc:svg": "node scripts/madge.js",
    "doc": "concurrently \"npm run doc:svg\" \"npm run doc:png\"",
    "generate:index": "node scripts/component-index.js src src/index.js > src/index.js",
    "generate:fixtures": "node scripts/fixture-tests.js src src/test/helpers/render-component.js",
    "generate:storybook": "node scripts/storybook-index.js src storybook/components.js > storybook/components.js",
    "generate": "concurrently \"npm run generate:index\" \"npm run generate:storybook\" \"npm run generate:fixtures\"",
    "init-component": "node scripts/init-component.js",
    "prepare": "npm run clean && npm run build --production",
    "prestart": "concurrently \"npm run generate:index\" \"npm run generate:storybook\"",
    "start": "npm run storybook:dev",
    "static": "npm run storybook:export",
    "storybook:dev": "cross-env BABEL_ENV=es start-storybook -p 3004 -c .storybook",
    "storybook:export": "cross-env BABEL_ENV=es build-storybook -c .storybook -o static"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>",
    "Gaelle Leguet <gaelle.leguet@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Joan Ortega <joan.ortega@coorpacademy.com>",
    "Julien Seren-Rosso <julien.seren-rosso@coorpacademy.com>",
    "Adrien Becchis <adrien.becchis@coorpacademy.com>"
  ],
  "dependencies": {
    "@coorpacademy/nova-icons": "1.1.18",
    "autoprefixer": "^7.1.1",
    "classnames": "^2.2.5",
    "css-color-function": "^1.3.0",
    "cubic-bezier": "^0.1.2",
    "extended-proptypes": "^1.3.0",
    "file-loader": "^0.11.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-modules-values-replace": "^1.2.1",
    "qs": "6.5.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-jw-player": "1.10.0"
  },
  "devDependencies": {
    "@coorpacademy/css-modules-require-hook": "0.0.2",
    "@coorpacademy/translate": "5.3.11",
    "@coorpacademy/webpack-config": "6.2.2",
    "@storybook/addon-knobs": "^3.2.15",
    "@storybook/addons": "^3.2.15",
    "@storybook/react": "^3.2.15",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "brandai-storybook": "^1.0.2",
    "browser-env": "^3.2.4",
    "change-case": "3.0.1",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "3.0.1",
    "glob": "^7.1.2",
    "json-loader": "^0.5.4",
    "madge": "^1.6.0",
    "minimist": "^1.2.0",
    "mkdirp-promise": "5.0.1",
