{
  "name": "@coorpacademy/components",
  "version": "7.4.3",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "locales",
    "postcss.config.js",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:build:commonjs": "rimraf lib",
    "clean:build:es": "rimraf es",
    "clean:build": "concurrently \"npm run clean:build:commonjs\" \"npm run clean:build:es\"",
    "clean": "npm run clean:build",
    "doc:png": "convert doc/components.svg doc/components.png",
    "doc:svg": "node scripts/madge.js",
    "doc": "concurrently \"npm run doc:svg\" \"npm run doc:png\"",
    "generate:fixtures": "node scripts/fixture-tests.js src src/test/helpers/render-component.js",
    "generate:storybook": "node scripts/storybook-index.js src storybook/components.js > storybook/components.js",
    "generate": "concurrently \"npm run generate:storybook\" \"npm run generate:fixtures\"",
    "prepare": "npm run clean && npm run build --production",
    "prestart": "npm run generate:storybook",
    "start": "npm run storybook:run",
    "static": "npm run storybook:export",
    "storybook:run": "cross-env BABEL_ENV=es start-storybook -p 3004 -c .storybook",
    "storybook:export": "cross-env BABEL_ENV=es build-storybook -c .storybook -o static"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>",
    "Gaelle Leguet <gaelle.leguet@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Joan Ortega <joan.ortega@coorpacademy.com>",
    "Julien Seren-Rosso <julien.seren-rosso@coorpacademy.com>",
    "Adrien Becchis <adrien.becchis@coorpacademy.com>"
  ],
  "dependencies": {
    "@coorpacademy/nova-icons": "1.2.3",
    "autoprefixer": "^8.4.1",
    "classnames": "^2.2.5",
    "css-color-function": "^1.3.0",
    "cubic-bezier": "^0.1.2",
    "extended-proptypes": "^1.3.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-modules-values-replace": "^3.0.1",
    "qs": "6.5.2",
    "react": "^16.3.2",
    "react-jw-player": "1.18.0"
  },
  "devDependencies": {
    "@coorpacademy/css-modules-require-hook": "1.0.0",
    "@coorpacademy/translate": "5.3.12",
    "@coorpacademy/webpack-config": "7.0.1",
    "@storybook/addon-knobs": "^3.3.0",
    "@storybook/addons": "^4.0.0-alpha.4",
    "@storybook/react": "^4.0.0-alpha.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "browser-env": "^3.2.4",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "madge": "^3.0.1",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.6.0",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.1",
    "rxjs": "^6.1.0",
    "style-loader": "^0.21.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.8.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "../../test/helpers/throw-warning"
    ],
    "babel": "inherit"
  },
  "author": "CoorpAcademy"
