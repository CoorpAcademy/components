{
  "name": "@coorpacademy/components",
  "version": "3.0.0-beta.3",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "start": "npm run generate:demo && BABEL_ENV=commonjs babel-node demo/index.js",
    "generate": "npm run generate:index && npm run generate:demo",
    "generate:index": "BABEL_ENV=commonjs babel-node scripts/generate-components-index.js > src/index.js",
    "generate:demo": "BABEL_ENV=commonjs babel-node scripts/generate-demo-index.js > demo/components.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:browser",
    "build:commonjs": "npm run clean:build:commonjs && BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "npm run clean:build:es && BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build:browser": "npm run clean:build:browser && BABEL_ENV=es webpack",
    "prepublish": "npm run build --production",
    "clean": "npm run clean:build && npm run clean:cov",
    "clean:build": "npm run clean:build:commonjs && npm run clean:build:es && npm run clean:build:browser",
    "clean:build:commonjs": "rm -rf lib",
    "clean:build:es": "rm -rf es",
    "clean:build:browser": "rm -rf dist/*",
    "clean:cov": "rm -rf .nyc_output coverage",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint src/",
    "ava": "NODE_ENV=test ava",
    "nyc": "NODE_ENV=test nyc npm run ava",
    "doc": "npm run doc:svg && npm run doc:png",
    "doc:svg": "node scripts/madge.js",
    "doc:png": "convert doc/components.svg doc/components.png"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "dependencies": {
    "@coorpacademy/treantjs-core": "^3.0.0-beta.3",
    "lodash": "^4.14.1"
  },
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^2.0.0",
    "@coorpacademy/translate": "^3.0.0-beta.3",
    "@coorpacademy/treantjs-engine-react": "^3.0.0-beta.3",
    "@coorpacademy/treantjs-engine-snabbdom": "^3.0.0-beta.3",
    "@coorpacademy/treantjs-engine-virtual-dom": "^3.0.0-beta.3",
    "api-check": "^7.5.5",
    "autoprefixer": "^6.4.1",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-babili": "0.0.3",
    "babel-preset-es2015": "^6.13.0",
    "babel-preset-stage-1": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "glob": "^7.0.5",
    "history": "^3.2.1",
    "hyperx": "^2.0.4",
    "json-loader": "^0.5.4",
    "madge": "^1.3.0",
    "nyc": "^8.1.0",
    "path-match": "^1.2.4",
    "postcss-loader": "^0.13.0",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "src/**/test/**"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  },
  "author": "CoorpAcademy"
}
