    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@coorpacademy/css-modules-require-hook": "2.0.3",
    "@coorpacademy/eslint-plugin-coorpacademy": "^9.0.1",
    "@coorpacademy/translate": "6.0.3",
