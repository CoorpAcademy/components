{
  "name": "@coorpacademy/components",
  "version": "9.30.2",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "locales",
    "postcss.config.js",
    "README.md"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib -s --ignore test --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es -s --ignore test --copy-files",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:commonjs": "rm -rf lib",
    "clean:es": "rm -rf es",
    "clean": "concurrently \"npm run clean:commonjs\" \"npm run clean:es\"",
    "doc:png": "convert doc/components.svg doc/components.png",
    "doc:svg": "node scripts/madge.js",
    "doc": "concurrently \"npm run doc:svg\" \"npm run doc:png\"",
    "generate:fixtures": "node scripts/fixture-tests.js src src/test/helpers/render-component.js",
    "generate:storybook": "node scripts/storybook-index.js src storybook/components.js > storybook/components.js",
    "generate": "concurrently \"npm run generate:storybook\" \"npm run generate:fixtures\"",
    "prepare": "npm run clean && npm run build --production",
    "prestart": "npm run generate:storybook",
    "start": "npm run storybook:run",
    "static": "npm run storybook:export",
    "storybook:run": "cross-env BABEL_ENV=es start-storybook -p 3004 -c .storybook",
    "storybook:export": "cross-env BABEL_ENV=es build-storybook -c .storybook -o static",
    "preava": "npm run generate:fixtures",
    "ava": "ava",
    "test:unit": "nyc npm run ava",
    "test": " npm run lint && npm run test:unit",
    "lint": "eslint . --ext .js,.json --cache --cache-location \"node_modules/.cache/.eslintcache\""
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "contributors": [
