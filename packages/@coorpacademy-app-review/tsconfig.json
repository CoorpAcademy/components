{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2020",
    "module": "commonjs",
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitOnError": false
  }
}
