{
  "name": "@coorpacademy/app-review",
  "version": "0.0.10",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "static": "node scripts/build.js",
    "poststatic": "cp -r public static",
    "prepare": "npm run clean && npm run build --production",
    "build:commonjs": "tsc -p tsconfig.lib.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:commonjs": "rm -rf lib",
    "clean:es": "rm -rf es",
    "clean": "concurrently \"npm run clean:commonjs\" \"npm run clean:es\"",
    "test:unit": "echo @todo setup unit tests"
  },
  "author": "CoorpAcademy",
  "files": [
    "lib",
    "es",
    "src",
    "readme.md"
  ],
  "main": "./es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "commonjs": "./lib/index.js",
      "types": "./es/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-native": "0.68.1",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/react-native": "^5.3.25",
    "chalk": "^5.0.1",
    "concurrently": "^5.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "esbuild": "^0.14.39",
    "esbuild-css-modules-plugin": "^2.2.16",
    "esbuild-plugin-svgr": "^1.0.1",
    "live-server": "^1.2.2",
    "react-dom": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^4.6.4"
  }
}
