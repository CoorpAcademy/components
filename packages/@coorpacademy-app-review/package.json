{
  "name": "@coorpacademy/app-review",
  "version": "0.2.1",
  "description": "",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "node commands/start.js",
    "static": "node commands/build-static-webapp.js",
    "poststatic": "cp -r public static",
    "prepare": "npm run clean && npm run build --production",
    "build:commonjs": "tsc -p tsconfig.lib.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "concurrently \"npm run build:commonjs\" \"npm run build:es\"",
    "clean:commonjs": "rm -rf lib",
    "clean:es": "rm -rf es",
    "clean": "concurrently \"npm run clean:commonjs\" \"npm run clean:es\"",
    "typecheck": "tsc --noEmit",
    "ava": "ava",
    "test:unit": "nyc npm run ava",
    "lint": "eslint '**/*' --cache --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run typecheck && npm run test:unit"
  },
  "author": "CoorpAcademy",
  "files": [
    "lib",
    "es",
    "src",
    "readme.md"
  ],
  "main": "./es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "commonjs": "./lib/index.js",
      "types": "./es/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@coorpacademy/components": "10.22.16",
    "@coorpacademy/redux-task": "^1.1.5",
    "cross-fetch": "^3.1.5",
    "jwt-decode": "^3.1.2",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@coorpacademy/css-modules-require-hook": "2.1.5",
    "@coorpacademy/eslint-plugin-coorpacademy": "^11.0.0",
    "@testing-library/react": "^12.1.5",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "ava": "^4.3.0",
    "browser-env": "^3.3.0",
    "chalk": "^4.1.2",
    "concurrently": "^5.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.39",
    "esbuild-css-modules-plugin": "https://github.com/chrisdugne/esbuild-css-modules-plugin#main",
    "esbuild-plugin-svgr": "^1.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "live-server": "^1.2.2",
    "nock": "^13.2.9",
    "postcss-modules-values-replace": "^3.4.0",
    "react-dom": "^17.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
