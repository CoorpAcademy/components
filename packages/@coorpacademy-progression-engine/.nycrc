{
  "include": [
    "src/**"
  ],
  "reporter": [
    "lcov",
    "text-summary",
  ],
    "exclude" : [
    "**/test/",
    "**/es/",
    "**/lib/",
  ],
  "check-coverage": true,
  "lines": 99,
  "statements": 99,
  "functions": 100,
  "branches": 92,
  "cache": true,
  "require": "babel-register",
  "sourceMap": false,
  "instrument": false,
  "all": true
}
