{
  "name": "@coorpacademy/components",
  "version": "1.0.4",
  "description": "Component library for web front ends",
  "main": "lib/components/index.js",
  "scripts": {
    "start": "babel-node sandbox/index.js",
    "build": "npm run build:commonjs && npm run build:browser",
    "build:commonjs": "npm run build:bundle && npm run clean:commonjs && BABEL_ENV=production babel src --out-dir lib -s",
    "build:browser": "npm run build:bundle && npm run clean:browser && webpack",
    "build:dev-mooc": "npm run build:bundle && npm run clean:browser && webpack --config webpack-dev-mooc.config.js",
    "build:bundle": "babel-node src/util/generate-bundle.js",
    "prepublish": "npm run build",
    "clean": "npm run clean:commonjs & npm run clean:browser & npm run clean:cov & wait",
    "clean:commonjs": "rm -rf lib",
    "clean:browser": "rm -rf dist",
    "clean:cov": "rm -rf .nyc_output coverage",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint src/",
    "ava": "BABEL_ENV=test ava",
    "nyc": "BABEL_ENV=test nyc ava --reporter=text --reporter=lcov",
    "coveralls": "BABEL_ENV=test nyc report --reporter=text-lcov | coveralls"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coorpacademy/components/issues"
  },
  "homepage": "https://github.com/coorpacademy/components#readme",
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^1.1.1",
    "angular": "^1.5.2",
    "autoprefixer": "^6.3.3",
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "^0.1.0",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.8.0",
    "compression-webpack-plugin": "^0.3.1",
    "coveralls": "^2.11.8",
    "css-loader": "^0.23.0",
    "eslint": "^3.1.1",
    "eslint-plugin-ava": "^2.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.3",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.14.1",
    "nyc": "^7.1.0",
    "pascal-case": "^1.1.2",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/bundle*",
      "src/components/widget*",
      "src/util/generation-bundle.js",
      "src/**/test/**"
    ],
    "require": [
      "babel-core/register"
    ],
    "all": false
  },
  "dependencies": {
    "api-check": "^7.5.5",
    "d3": "^3.5.16",
    "react": "^15.0.2",
    "react-dom": "^15.3.0",
    "snabbdom": "^0.5.1",
    "virtual-dom": "^2.1.1",
    "nvd3": "^1.8.2"
  }
}
