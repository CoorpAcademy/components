{
  "name": "@coorpacademy/components",
  "version": "0.0.1",
  "description": "Component library for web front ends",
  "main": "index.js",
  "scripts": {
    "start": "node sandbox/index.js",
    "build": "npm run build:commonjs & npm run build:browser & wait",
    "build:commonjs": "npm run clean:commonjs && babel src --out-dir lib -s",
    "build:browser": "npm run clean:browser && webpack",
    "clean": "npm run clean:commonjs & npm run clean:browser & wait",
    "clean:commonjs": "rm -rf lib",
    "clean:browser": "rm -rf dist",
    "test": "npm run lint && npm run ava",
    "example:angular": "node example/angular/index.js",
    "lint": "eslint src/",
    "ava": "ava"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coorpacademy/components/issues"
  },
  "homepage": "https://github.com/coorpacademy/components#readme",
  "devDependencies": {
    "angular": "^1.4.8",
    "ava": "^0.12.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-1": "^6.3.13",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.1.0",
    "eslint-plugin-ava": "0.0.1",
    "eslint-plugin-jsx": "0.0.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "lodash.defaultsdeep": "^4.0.4",
    "lodash.isfunction": "^3.0.6",
    "lodash.isstring": "^4.0.1",
    "lodash.last": "^3.0.0",
    "lodash.mapvalues": "^3.0.1",
    "lodash.noop": "^3.0.0",
    "lodash.omit": "^4.1.0",
    "lodash.partial": "^4.0.1",
    "postcss-color-function": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "style-loader": "^0.13.0",
    "virtual-dom": "^2.1.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "ava": {
    "files": [
      "**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ]
  }
}
