{
  "name": "@coorpacademy/components",
  "version": "0.2.2",
  "description": "Component library for web front ends",
  "main": "lib/index.js",
  "scripts": {
    "start": "node sandbox/index.js",
    "build": "npm run build:commonjs & npm run build:browser & wait",
    "build:commonjs": "npm run clean:commonjs && babel src --out-dir lib -s",
    "build:browser": "npm run clean:browser && webpack",
    "build:dev-mooc": "npm run clean:browser && webpack --config webpack-dev-mooc.config.js",
    "prepublish": "npm run build",
    "clean": "npm run clean:commonjs & npm run clean:browser & npm run clean:cov & wait",
    "clean:commonjs": "rm -rf lib",
    "clean:browser": "rm -rf dist",
    "clean:cov": "rm -rf .nyc_output coverage",
    "test": "npm run lint && npm run nyc",
    "example:angular": "node example/angular/index.js",
    "lint": "eslint src/",
    "ava": "ava",
    "nyc": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "contributors": [
    "Arthur Weber <arthur.weber@coorpacademy.com>",
    "Jeroen Engels <jengels@coorpacademy.com>",
    "Christophe Dugne-Esquevin <christophe.dugne@coorpacademy.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coorpacademy/components/issues"
  },
  "homepage": "https://github.com/coorpacademy/components#readme",
  "devDependencies": {
    "angular": "^1.5.2",
    "autoprefixer": "^6.3.3",
    "ava": "^0.14.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "coveralls": "^2.11.8",
    "css-loader": "^0.23.0",
    "eslint": "^2.5.3",
    "eslint-plugin-ava": "^2.2.1",
    "eslint-plugin-jsx": "0.0.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "nyc": "^6.1.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "style-loader": "^0.13.0",
    "virtual-dom": "^2.1.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": ["src/**"],
    "require": ["babel-core/register"],
    "all": true
  },
  "dependencies": {
    "d3": "^3.5.16",
    "nvd3": "^1.8.2"
  }
}
