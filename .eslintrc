{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "angular": true
  },
  "extends": [
    "plugin:@coorpacademy/coorpacademy/core"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "@coorpacademy/coorpacademy"
  ],
  "rules": {
    "no-unused-vars": 'off'
  }
}
